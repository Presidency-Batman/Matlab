MATLAB CODE 1
clc
clear
close all
n=input('enter the order of matrix');
disp('enter thje eelements of coefficient matrix');
for i=l:n
 for j=l:n
A(i,j)=input('');
end
end
determinant=det(A)
if det(A)!=0
disp('inverse of matrix A is')
inverse=inv(A)
else
disp('A is a singular matrix and inverse does not exist');
end




MATLAB CODE 2
clc 
clear 
close all
n=input('enter the number of equations');
m=input('enter the  number of unknowns');
disp('enter the elemeents of matrix');
for i=1:n
 for j=1:n
  A(i,j)=input('');
end
end
disp('enter the constant matrix');
for i=1:n
c(i)=input('')
end
B=C'
if n!=m
disp('the number of equations are not equal to number of unknowns hence it is not 
possible to find solution by matrix inversion method');

else if B==0
disp('this is homogenous system of equations');
disp('it has trival or infinitely many solutions');

else if det(A)==0
disp('it is not possible to find solution by matrix inversion method');

else
disp('the solution is:');
X=inv(A)*B
end




MATLAB CODE 3
clc
clear
close all
n=input('enter the order of square matrix');
disp('enter the elements of coefficient matrix');
for i=1:n
 for j=1:n
  A(i,j)=input('');
end
end
trace=trace(A)
determinant=det(A)
disp('the characterstic equation is');
p=poly(A)
disp('the eigen values are');
eigenvalues=eig(A)
disp('the eigen values are');
[X,D]=eig(A)





MATLAB CODE 4
clc
clear 
close all
syms x
disp('the function is');
f=sin(exp(sin(2*x)))
disp('the derivative of the function is');
derivative=diff(f,x)





MATLAB CODE 5 (TAYLORS SERIES)
i)SINGLE VARIABLE

clc
clear
close all
syms x
t1=taylor(exp(x))
t2=taylor(sin(x))
t3=taylor(log(sin(x)),x,2)
t4=taylor(log(x),x,'expansion point',2)

ii)TWO VARIABLE

clc
clear
close all
syms x y
f=(x^2)*(y+3)*(y-2)
t1=taylor(f,[x,y],[1,-2],'order',10)
pretty(t1)





MATLAB CODE 6(MAXIMA ND MINIMA)
clc
clear
close all
syms x y real
f=input('enter the function f(x,y):');
fx=diff(f,x);
fy=diff(f,y);
[X,Y]=solve(fx==0,fy==0,[x,y]);
A=diff(fx,x);
B=diff(fx,y);
C=diff(fy,y);
D=A*C-B^2;

for i=1:length(x)
d=subs(D,{x,y},{X(i),Y(i)});
a=subs(A,(x,y},{X(i),Y(i)});
f_value=subs(f,{x,y},{X(i),Y(i)});

if d>0 and a<0
fprintf('maximum at a point (%d,%d)and maximum value is %d\n',[X(i),Y(i)],f_value);

else if d>0 and a>0
fprintf('minimum at a point (%d,%d) and minimum value is %d\n',[X(i),Y(i)],f_value);

else if d<0
fprintf('sadde point is (%d,%d)\n'[X(i),Y(i)]);

else
fprintf('no conclusion can be drawn for (%d,%d)\n',[X(i),Y(i)];
end
end





MATLAB CODE 7
clc
clear
close all
syms x;
f=x^2*cos(x);
area=int(f,-4,9)
ezplot(f[-4,9])




MATLAB CODE 8
clc
clear 
close all
disp('the cauchy equation is');
syms y(x)
ode=2*x^2*diff(y,x,2)+3*x*dff(y,x)-y==0
disp('solution of given equation');
ysol(x)=dsolve(ode)
