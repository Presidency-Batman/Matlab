MATLAB CODE 1
clc
clear
close all
n=input('enter the order of matrix');
disp('enter thje eelements of coefficient matrix');
for i=l:n
 for j=l:n
A(i,j)=input('');
end
end
determinant=det(A)
if det(A)!=0
disp('inverse of matrix A is')
inverse=inv(A)
else
disp('A is a singular matrix and inverse does not exist');
end
